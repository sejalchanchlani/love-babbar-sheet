int repeatedSubtraction(int a,int b){
        //code here
         int c=0;
        while(1)
        {
           
            if(b>a)
            {
                b=b-a;
                c++;
            }
            else
            {
                a=a-b;
                c++;
            }
            if(a==0 || b==0)
            {
                return c;
            }
        }
