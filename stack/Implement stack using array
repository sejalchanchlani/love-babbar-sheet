void MyStack :: push(int x)
{
        // Your Code
        top=top+1;
        arr[top]=x;
}

/*The method pop which return the element 
  poped out of the stack*/
int MyStack :: pop()
{
        // Your Code   
        if(top==-1)
        {
            return -1;
        }
        else
        {
            return arr[top--];
        }
}
